            "description": "Фильтр по статусу верификации проекта",
            "schema": {
              "example": true,
              "type": "boolean"
            }
          },
          {
            "name": "isBlocked",
            "required": false,
            "in": "query",
            "description": "Фильтр по статусу блокировки проекта",
            "schema": {
              "example": false,
              "type": "boolean"
            }
          }
        ],
