  "components": {
    "securitySchemes": {
      "JWT-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "RegisterDto": {
        "type": "object",
        "properties": {
          "organizationName": {
            "type": "string",
            "description": "Organization name",
            "example": "My Organization",
            "minLength": 1,
            "maxLength": 100
          },
          "lastName": {
            "type": "string",
            "description": "User last name",
            "example": "Doe",
            "minLength": 2,
            "maxLength": 20
          },
          "firstName": {
            "type": "string",
            "description": "User first name",
            "example": "John",
            "minLength": 2,
            "maxLength": 20
          },
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "john.doe@example.com",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User password (6 to 20 characters)",
            "example": "password123",
            "minLength": 6,
            "maxLength": 20,
            "format": "password"
          }
        },
        "required": [
          "organizationName",
          "lastName",
          "firstName",
          "email",
          "password"
        ]
      },
      "RegisterResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "description": "HTTP status code",
            "example": 200
          },
          "message": {
            "type": "string",
            "description": "Success registration message",
            "example": "User registered successfully"
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "ErrorResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "description": "HTTP status code",
            "example": 400
          },
          "message": {
            "type": "object",
            "description": "Error message",
            "example": "Validation failed"
          },
          "error": {
            "type": "string",
            "description": "Error name",
            "example": "Bad Request"
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "john@example.com",
            "format": "email"
          },
          "password": {
            "type": "string",
            "description": "User password (6 to 20 characters)",
            "example": "password123",
            "minLength": 6,
            "maxLength": 20,
            "format": "password"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "description": "HTTP status code",
            "example": 200
          },
          "message": {
            "type": "string",
            "description": "Success login message",
            "example": "User logged in successfully"
          }
        },
        "required": [
          "statusCode",
          "message"
        ]
      },
      "RefreshTokenResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "Success token refresh message",
            "example": "Token refreshed successfully"
          }
        },
        "required": [
          "message"
        ]
      },
      "LogoutResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "Success logout message",
            "example": "Logout successful"
          }
        },
        "required": [
          "message"
        ]
      },
      "UpdateProfileDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "User ID",
            "example": "123e4567-e89b-12d3-a456-426614174000"
